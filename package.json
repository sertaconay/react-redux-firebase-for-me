  "scripts": {
    "prestart": "yarn --force",
    "start": "npm run build:clean && npm-run-all --parallel copy:assets:watch server:dev",
    "server:dev": "babel-node tools/server/dev.js",
    "server:prod": "babel-node tools/server/prod.js",
    "copy:assets": "cp -rv src/assets/images public",
    "copy:assets:watch": "watch \"cp -rv src/assets/images public\" src/assets/images",
    "prebuild": "npm-run-all build:clean build:html",
    "build": "babel-node tools/build/project.js",
    "build:clean": "rm -rf public && mkdir public",
    "build:html": "babel-node tools/build/html.js",
    "predeploy": "yarn --force && npm run build:clean && npm run copy:assets && cross-env NODE_ENV=production npm run build:html && cross-env NODE_ENV=production webpack",
    "deploy": "firebase deploy"
  },
